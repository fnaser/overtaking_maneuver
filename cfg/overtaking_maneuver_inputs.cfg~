#!/usr/bin/env python
PACKAGE = "overtaking_maneuver"

from dynamic_reconfigure.parameter_generator_catkin import *

gen = ParameterGenerator()

gen.add("input_vel",     double_t, 0, "Initial and final velocity [m/s]",                                       0, 0, 1)
gen.add("input_width",   double_t, 0, "Width of the lane or of the diversion [m]",                              0, 0, 1)
gen.add("input_max_acc", double_t, 0, "Magnitude of the maximal resultant acceleration of ego vehicle [m/s^2]", 0, 0, 1)

exit(gen.generate(PACKAGE, "overtaking_maneuver", "overtaking_maneuver_inputs"))
